!!!5
html
  head
    title NGN APL
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no')
    style: @font-face { font-family: apl; src: url(../fonts/apl385.ttf) format(truetype); }
    style: @import url(index.css);

  body

    #pageInput
      #editor: img#cursor(src='images/cursor.png')
      table.keyboard
        tr
          - for (var i = 0; i < 10; i++)
              td.key(colSpan=2)
        tr
          - for (var i = 0; i < 10; i++)
              td.key(colSpan=2)
        tr
          td
          - for (var i = 0; i < 9; i++)
              td.key(colSpan=2)
          td
        tr
          td.key.special.shift(colSpan=3) Shift
          - for (var i = 0; i < 7; i++)
              td.key(colSpan=2)
          td.key.special.bksp.repeatable(colSpan=3) Bksp
        tr
          td.key.special.alt(colSpan=3) Alt
          td.key.special.space(colSpan=10)
          td.key.special.enter(colSpan=3) Enter
          td.key.special.exec(colSpan=4) Exec

      script(src='../web/examples.js')
      script(src='../web/jquery-1.8.2.min.js')
      script(src='../web/require.js', data-main='./index.js')
      script
        var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
        document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
      script
        var pageTracker = _gat._getTracker("UA-23946726-1");
        pageTracker._initData();
        pageTracker._trackPageview();

    #pageOutput(style='display:none')
      button#closeOutputButton Close
      #result
